{
  "publisher": "antfu",
  "name": "json-discovery",
  "displayName": "JSON Discovery",
  "version": "0.1.1",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "description": "VS Code integration for JsonDiscovery to inspect JSON files.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vscode-json-discovery#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/vscode-json-discovery"
  },
  "bugs": {
    "url": "https://github.com/antfu/vscode-json-discovery/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/antfu"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist",
    "res"
  ],
  "engines": {
    "vscode": "^1.97.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "menus": {
      "editor/title": [
        {
          "command": "json-discovery.show",
          "when": "editorLangId == json",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "json-discovery.show",
        "title": "Show in JSON Discovery",
        "category": "JSON Discovery",
        "icon": "$(telescope)"
      }
    ],
    "configuration": []
  },
  "scripts": {
    "build": "nr build:view && tsup src/index.ts --external vscode",
    "build:view": "tsx scripts/build-view.mts",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "update": "vscode-ext-gen",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "git submodule update --init --recursive --depth=1",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@antfu/ni": "^23.3.1",
    "@discoveryjs/discovery": "1.0.0-beta.91",
    "@types/node": "^22.13.4",
    "@types/vscode": "^1.97.0",
    "@vscode/vsce": "^3.2.2",
    "bumpp": "^10.0.3",
    "esbuild": "^0.24.0",
    "eslint": "^9.20.1",
    "esno": "^4.8.0",
    "jszip": "^3.10.1",
    "pnpm": "^10.4.1",
    "rimraf": "^6.0.1",
    "tinyexec": "^0.3.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vite-plugin-singlefile": "^2.1.0",
    "vitest": "^3.0.6",
    "vscode-ext-gen": "^1.0.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
